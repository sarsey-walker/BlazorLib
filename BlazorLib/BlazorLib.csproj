<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageId>WBlazorLib</PackageId>
		<Version>0.1.0-alpha1</Version>
		<Authors>Will Walker</Authors>
		<Copyright>Copyright 2023 Will Walker</Copyright>
		<PackageTags>Blazor, Components, Blazor Components, Blazor Library</PackageTags>
		<Description>This is a Razor component library with components used by myself on my projects.</Description>
		<RepositoryUrl>https://github.com/sarsey-walker/BlazorLib</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>


	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="8.0.0" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="wwwroot\" />
	</ItemGroup>

</Project>
